<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chyngyz.quizapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".QuizApp"
        android:allowBackup="true"
        android:icon="@drawable/vec"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizApp">
        <activity android:name=".cloud_masseging.PushNotifActivity"></activity>
        <activity android:name=".ui.settingsFragment.settings.SettingsActivity" />
        <activity android:name=".ui.resultActivity.ResultActivity" />
        <activity android:name=".ui.question.QuestionActivity" />
        <activity android:name=".SecActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".cloud_masseging.FBcloudMassaging"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>